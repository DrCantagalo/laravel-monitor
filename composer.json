{
    "name": "DrCantagalo/laravel-monitor",
    "description": "A lightweight Laravel package providing CRM tools, access monitoring, and anti-scraper protection.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michel Cantagalo",
            "email": "cantagalo@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Monitor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Monitor\\MonitorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}